<p id="notice"><%= notice %></p>

<h1>Tasks Logs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tasks_logs.each do |tasks_log| %>
      <tr>
        <td><%= link_to 'Show', tasks_log %></td>
        <td><%= link_to 'Edit', edit_tasks_log_path(tasks_log) %></td>
        <td><%= link_to 'Destroy', tasks_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tasks Log', new_tasks_log_path %>
